@article{abu-aliDeepLearningBasedLongHorizon2022,
  title = {Deep {{Learning-Based Long-Horizon MPC}}: {{Robust}}, {{High Performing}}, and {{Computationally Efficient Control}} for {{PMSM Drives}}},
  shorttitle = {Deep {{Learning-Based Long-Horizon MPC}}},
  author = {{Abu-Ali}, Mohammad and Berkel, Felix and Manderla, Maximilian and Reimann, Sven and Kennel, Ralph and Abdelrahem, Mohamed},
  year = 2022,

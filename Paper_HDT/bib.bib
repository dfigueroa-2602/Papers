@article{ufnalskiRobustTuningMultiresonant2022,
  title = {Robust {{Tuning}} of {{Multiresonant Current Controllers}} for {{Grid-Tied Converters}} and {{Erroneous Use}} of the {{Naslin Polynomial Method}}},

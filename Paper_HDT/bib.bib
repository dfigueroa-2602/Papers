@article{bimartaFrequencyAdaptiveCurrentController2018,
  title = {Frequency-{{Adaptive Current Controller Design Based}} on {{LQR State Feedback Control}} for a {{Grid-Connected Inverter}} under {{Distorted Grid}}},
  author = {Bimarta, Rizka and Tran, Thuy Vi and Kim, Kyeong-Hwa},
